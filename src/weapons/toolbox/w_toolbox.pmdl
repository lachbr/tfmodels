{
  model "optchar/w_toolbox-zero.egg"

  hpr [ 180 0 0 ]

  lods [
    {
      distance 0
      groups [ w_toolbox_reference ]
    }
    {
      distance 14
      groups [ w_toolbox_reference_lod1 ]
    }
  ]

  material_paths [
    "../../materials"
  ]

  material_groups [
    {
      name red
      materials [ "toolbox_red.pmat" ]
    }
    {
      name blue
      materials [ "toolbox_blue.pmat" ]
    }
  ]

  physics_model {
    mesh w_toolbox_physics
    mass 1.0
  }
}
